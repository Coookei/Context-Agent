    "Node.js",
    "Express",
    "TypeScript",
    "OpenAI",
    "AI Agents",
    "Bun",
    "Vite",
    "Husky"
  ],
  "author": "Coookei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Coookei/Context-Agent.git"
  },
  "scripts": {
    "dev": "bun run index.ts",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "concurrently": "^9.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "workspaces": [
    "packages/*"
